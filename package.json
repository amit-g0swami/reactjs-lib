{
  "version": "0.0.1",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "start": "tsdx watch",
    "start-dev": "FAST_REFRESH=false react-scripts start",
    "build": "npm run compile",
    "compile": "tsdx build",
    "test": "tsdx test --maxWorkers=4",
    "test:watch": "tsdx test --watch",
    "coverage": "tsdx test --coverage --maxWorkers=4",
    "lint": "tsdx lint src",
    "lint-fix": "tsdx build",
    "size": "size-limit",
    "prettier:check": "prettier --check \"**/*.{css,js,json,jsx,ts,tsx}\"",
    "prettier:fix": "prettier --write \"**/*.{css,js,json,jsx,ts,tsx}\"",
    "prettier-all": "npm run prettier:check && npm run prettier:fix",
    "precompile": "npm run prettier-all",
    "postbuild": "npx tailwindcss -i ./src/theme/style.css -o ./dist/theme/output.css"
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "tabWidth": 2
  },
  "name": "@amit_g0swami/reactjs-lib",
  "author": "amit_g0swami",
  "module": "dist/reactjs-lib.esm.js",
  "size-limit": [
    {
      "path": "dist/reactjs-lib.cjs.production.min.js",
      "limit": "200 KB"
    },
    {
      "path": "dist/reactjs-lib.esm.js",
      "limit": "200 KB"
    }
  ],
  "peerDependencies": {
    "react": ">=18",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.17",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@emotion/react": "^11.11.1",
    "@storybook/addon-essentials": "^7.4.1",
    "@storybook/addon-interactions": "^7.4.1",
    "@storybook/addon-links": "^7.4.1",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/addon-styling-webpack": "^1.0.0",
    "@storybook/blocks": "^7.4.1",
    "@storybook/react": "^7.4.1",
    "@storybook/react-webpack5": "^7.4.1",
    "@storybook/testing-library": "^0.2.0",
    "@types/mdast": "^4.0.0",
    "@types/react": "^18.2.21",
    "@types/react-datepicker": "^4.15.0",
    "@types/react-dom": "^18.2.7",
    "babel-loader": "^9.1.3",
    "prettier": "^3.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-icons": "^4.11.0",
    "react-scripts": "^5.0.1",
    "size-limit": "^9.0.0",
    "storybook": "^7.4.1",
    "tailwindcss": "^3.3.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "axios": "^1.5.0",
    "date-fns": "^3.6.0",
    "joi": "^17.10.1"
  },
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amit-g0swami/reactjs-lib.git"
  },
